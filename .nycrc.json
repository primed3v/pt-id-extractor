{
    "cache": false,
    "check-coverage": false,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts"
    ],
    "exclude": [
      "src/environments/*.ts"
    ],
    "reporter": [
      "cobertura",
      "text-summary"
    ],
    "statements": 90,
    "branches": 90,
    "functions": 90,
    "lines": 90
}